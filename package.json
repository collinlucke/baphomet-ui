{
  "name": "baphomet-ui",
  "version": "1.5.0",
  "type": "module",
  "scripts": {
    "dev": "bash scripts/dev-with-cleanup.sh",
    "dev:windows": "powershell -ExecutionPolicy Bypass -File scripts/dev-with-cleanup.ps1",
    "dev:simple": "vite",
    "test": "vitest",
    "build": "echo 'Starting build...' && tsc --diagnostics && echo 'TypeScript complete' && vite build --debug --logLevel info",
    "lint": "eslint src/ --report-unused-disable-directives --max-warnings 50",
    "lint:fix": "eslint . --report-unused-disable-directives --max-warnings 50 --fix",
    "preview": "vite preview",
    "install:dev": "pnpm install && pnpm link ../phantomartist",
    "install:prod": "pnpm install"
  },
  "dependencies": {
    "@apollo/client": "^4.0.4",
    "phantomartist": "1.4.1",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/styled": "^11.14.1",
    "@floating-ui/react": "^0.27.16",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitest/browser": "^3.2.4",
    "@vitest/runner": "^3.2.4",
    "apollo-upload-client": "^19.0.0",
    "dotenv": "^17.2.2",
    "graphql": "^16.11.0",
    "hugeicons-react": "^0.3.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "party-js": "^2.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rollup-plugin-delete": "^3.0.1",
    "vite-plugin-static-copy": "^3.1.2"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@eslint/js": "^9.35.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "react-router-dom": "^7.8.2"
  }
}
