{
  "name": "baphomet-ui",
  "basePath": "/baphomet",
  "build": {
    "outputDirectory": "dist",
    "commands": [
      "pnpm install",
      "pnpm run build"
    ]
  },
  "routing": {
    "rules": [
      {
        "source": "/baphomet/**",
        "destination": "/baphomet/index.html",
        "type": "rewrite"
      }
    ]
  }
}
